include ../mixins/_mixins
doctype html

html
    head
        // Meta Settings
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        meta(charset="utf-8")
        block meta

        // CSS
        block style
            +style("assets/css/min.css")

        block title
    body
        block container
            #container

        block script
            +script("assets/js/jquery.min.js")